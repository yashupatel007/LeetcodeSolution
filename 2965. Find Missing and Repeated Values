class Solution {
public:
    vector<int> findMissingAndRepeatedValues(vector<vector<int>>& grid) {
        long long  n=grid.size();
        vector<int>res;
        unordered_map<int,int>mp;
        long long sum=0;
        long long grid_sum=0;
        long long square=n*n;

        sum=square*(square+1)/2;

        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                grid_sum+=grid[i][j];

                if(mp.count(grid[i][j])==0){
                    mp[grid[i][j]]=1;
                }
                else {
                    res.push_back(grid[i][j]);
                }
            }
        }
        int value=sum-(grid_sum-res[0]);
        res.push_back(value);
        return res;
    }
};
