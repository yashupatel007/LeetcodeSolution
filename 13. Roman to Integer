class Solution {
public:
int value(char c){
    if(c=='I')
    return 1;
    else if(c=='V')
    return 5;
     else if(c=='X')
    return 10;
     else if(c=='L')
    return 50;
     else if(c=='C')
    return 100;
     else if(c=='D')
    return 500;
     else 
    return 1000;
}

    int romanToInt(string s) {
        int sum=0;
        for(int i=0;i<s.size()-1;i++){
            if(value(s[i+1])>value(s[i]))
               sum-=value(s[i]);
            else 
             sum+=value(s[i]);
        }
        sum+=value(s[s.size()-1]);
        return sum;
    }

};
