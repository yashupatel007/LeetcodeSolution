class Solution {
public:
    int minOperations(vector<int>& nums, int k) {
        int n=nums.size();
        int sum=0;
        
        for(int i=0;i<n;i++){
            sum+=nums[i];
        }
        int index=sum;
        int count=0;
        while(index%k!=0){
             index--;
             count++;
        }
        return count;
    }
};
