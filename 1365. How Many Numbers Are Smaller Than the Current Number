class Solution {
public:
    vector<int> smallerNumbersThanCurrent(vector<int>& nums) {
        vector<int>store=nums;
        int n=nums.size();
        
        sort(nums.begin(),nums.end());
        unordered_map<int,int>mp;

        for(int i=0;i<n;i++){
            if(mp.count(nums[i])==0)
                mp[nums[i]]=i;
        }
        for(int i=0;i<n;i++){
            store[i]=mp[store[i]];
        }

        return store;
    }
};
