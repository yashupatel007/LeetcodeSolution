class Solution {
public:
    int repeatedNTimes(vector<int>& nums) {
        int p=0;
        int n=nums.size();
        unordered_map<int,int>mp;
        for(int i=0;i<n;i++){
            if(mp.count(nums[i])==0)
               mp[nums[i]]=1;
            else {
                 p=nums[i];
                 break;
            }
        }
        return p;
        // //approach 2nd(agar n+1 element baar repeat
        // // ho raha to kafi dense ho jayega dekhne me gap 1 ya 2 hoga)
        // for(int i=0;i<n-2;i++){
        //     if(nums[i]==nums[i+1])
        //       return nums[i];
        //     else if(nums[i]==nums[i+2])
        //       return nums[i];
        // }
        // //if not found edge case [x,a,b,x]
        // return nums[nums.size()-1];
        
    }
};
